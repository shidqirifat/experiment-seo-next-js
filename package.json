{
  "name": "starterkit-react-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format:all": "prettier --ignore-path .gitignore  --write \"**/*.+(js|ts|jsx|tsx)\"",
    "format:dirty": "pretty-quick --ignore-path .gitignore --pattern \"**/*.+(js|ts|jsx|tsx)\"",
    "format:check": "prettier --ignore-path .gitignore --check \"**/*.+(js|ts|jsx|tsx)\"",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.1",
    "@hookform/resolvers": "^3.3.4",
    "@internationalized/date": "^3.5.1",
    "@types/node": "20.11.2",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "10.4.16",
    "axios": "^1.6.7",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.4",
    "next": "14.0.4",
    "next-international": "^1.2.3",
    "postcss": "8.4.33",
    "react": "^18",
    "react-aria": "^3.31.1",
    "react-dom": "^18",
    "react-stately": "^3.29.1",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.3.3"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-storybook": "^0.6.15",
    "prettier": "^3.2.2",
    "prettier-plugin-tailwindcss": "^0.5.11"
  }
}
